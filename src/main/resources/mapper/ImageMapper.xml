<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ysj.ysj_free.mapper.ImageMapper">
    <insert id="saveImage" parameterType="com.ysj.ysj_free.domain.Image">
        INSERT INTO images (id, fileName, filePath, createdDate)
        VALUES (#{id}, #{fileName}, #{filePath}, #{createdDate})
    </insert>
</mapper>